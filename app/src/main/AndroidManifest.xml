<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.freshapp" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/mystyle">
        <activity android:name=".DrawPathSvgActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Favorite_Activity"
            />
        <activity
            android:name=".MainActivity"
            />
        <activity
            android:name=".Content"
            android:label="@string/title_activity_content" >
        </activity>
        <activity
            android:name=".SwipBack_Content"
             >
        </activity>
        <activity
            android:name="cn.smssdk.SMSSDKUIShell"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@android:style/Theme.Translucent.NoTitleBar"
            android:windowSoftInputMode="stateHidden|adjustResize" />
        <activity
            android:name=".loginUtil.verifyCode_Activity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name=".loginUtil.setPassword_Activity"
            android:label="@string/title_activity_set_password_" >
        </activity>
        <activity
            android:name=".loginUtil.test"
            android:label="@string/title_activity_test" >
        </activity>
        <activity android:name=".loginUtil.login_Activity" >
        </activity>
        <activity
            android:name=".UserActivity"
            android:label="@string/title_activity_user"

            >
        </activity>
    </application>

</manifest>
